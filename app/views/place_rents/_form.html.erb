<%= form_for([@parking, @place_rent]) do |f| %>
  <% if @parking.errors.any? || @place_rent.errors.any? %>
    <div id="error_explanation">
      <div class="alert alert-danger">
        The form contains <%= pluralize(@parking.errors.count + @place_rent.errors.count, "error") %>.
      </div>
      <ul>
      <% @parking.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      <% @place_rent.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="form-group">
    <%= f.label :starts_at %>
    <div class="form-inline">
      <%= f.datetime_select :starts_at, {}, {class: "form-control"} %>
    </div>
  </div>

  <div class="form-group">
    <%= f.label :ends_at %>
    <div class="form-inline">
      <%= f.datetime_select :ends_at, {}, {class: "form-control"} %>
    </div>
  </div>

  <div class="form-group">
    <%= f.label :car_id %>
    <%= f.collection_select(:car_id, current_person.cars.order(:model), :id, :model, include_blank: false) %>
  </div>

  <div class="form-group">
    <%= f.submit class: "form-control" %>
  </div>
<% end %>
