<h1>Cars</h1>
<%= link_to "New car", new_car_path %>
<table>
  <tr>
    <th>Model</th>
    <th>Registration number</th>
    <th colspan="3"></th>
  </tr>
  <% @cars.each do |car| %>
  <tr>
    <td><%= car.model %></td>
    <td><%= car.registration_number %></td>
    <td><%= link_to "Show", car_path(car) %></td>
    <td><%= link_to "Edit", edit_car_path(car) %></td>
    <td><%= link_to "Remove", car_path(car), method: :delete %></td>
  </tr>
  <% end %>
</table>
